$ZoneId  = "YOUR_REAL_ZONE_ID"
$headers = @{ Authorization = "Bearer $ApiToken" }

Invoke-RestMethod -Method GET `
  -Uri "https://api.cloudflare.com/client/v4/zones/$ZoneId" `
  -Headers $headers





$baseUrl = "https://api.cloudflare.com/client/v4/zones/$ZoneId"
Invoke-RestMethod -Method GET -Uri "$baseUrl/rulesets" -Headers $headers
